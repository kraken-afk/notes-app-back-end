{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "quotes": [
            "error",
            "double",
            {
                "allowTemplateLiterals": true
            }
        ],
        "semi": [
            "error",
            "always"
        ],
        "max-depth": [
            "error",
            3
        ],
        "max-params": [
            "error",
            3
        ],
        "no-var": [
            "error"
        ],
        "no-unused-vars": [
            "warn"
        ]
    }
}
